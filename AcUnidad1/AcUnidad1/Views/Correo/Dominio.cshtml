@{
}

@{
    ViewData["Title"] = "Dominio del Correo";
}

<h2>Dominio del Correo Electrónico</h2>

@if (ViewBag.Mensaje != null)
{
    <p>@ViewBag.Mensaje</p>
}
else
{
    <p>El correo <strong>@ViewBag.Correo</strong> tiene el dominio: <strong>@ViewBag.Dominio</strong></p>
}