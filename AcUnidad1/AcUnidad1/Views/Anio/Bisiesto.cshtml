@{
}
@{
    ViewData["Title"] = "Año Bisiesto";
}

<h2>Verificación de Año Bisiesto</h2>

@if (ViewBag.EsBisiesto)
{
    <p>El año <strong>@ViewBag.Anio</strong> es bisiesto.</p>
}
else
{
    <p>El año <strong>@ViewBag.Anio</strong> no es bisiesto.</p>
}