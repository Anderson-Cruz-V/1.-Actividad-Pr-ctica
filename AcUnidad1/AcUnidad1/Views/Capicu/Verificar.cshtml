@{
}
@{
    ViewData["Title"] = "Verificación de Capicúa";
}

<h2>Verificación de Capicúa</h2>

@if (ViewBag.EsCapicua)
{
    <p>El número <strong>@ViewBag.Numero</strong> es capicúa.</p>
}
else
{
    <p>El número <strong>@ViewBag.Numero</strong> no es capicúa.</p>
}