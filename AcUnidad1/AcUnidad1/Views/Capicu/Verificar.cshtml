@{
}
@{
     ViewData["Title"] = "Verificación de Capicúa";
}

<h3>Verificación de Capicú</h3>

@if (ViewBag.EsCapicua)
{
    <h1>El número <strong>@ViewBag.Numero</strong> es capicú.</h1>
}
else
{
    <h1>El número <strong>@ViewBag.Numero</strong> no es capicú.</h1>
}